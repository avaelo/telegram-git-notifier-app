{
  "name": "tanhongit/telegram-bot-github-notify",
  "description": "A Telegram bot send notify Github events to Telegram channels",
  "keywords": [
    "telegram",
    "bot",
    "github",
    "notify",
    "events",
    "webhook",
    "telegram-notifier",
    "github-notifier",
    "github-telegram-bot"
  ],
  "homepage": "https://github.com/tanhongit/telegram-bot-github-notify",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "eleirbag89/telegrambotphp": "^1.4",
    "guzzlehttp/guzzle": "^7.7",
    "symfony/http-foundation": "^6.3",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "TelegramGithubNotify\\App\\": "src/"
    },
    "files": [
      "common/helpers.php"
    ]
  },
  "authors": [
    {
      "name": "Tan Nguyen",
      "email": "tannp27@gmail.com",
      "homepage": "https://tanhongit.com",
      "role": "Lead"
    }
  ],
  "support": {
    "issues": "https://github.com/tanhongit/telegram-bot-github-notify/issues"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
